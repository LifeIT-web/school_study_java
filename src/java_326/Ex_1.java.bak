public class Ex_1 
{
	public static void main(String[] args) 
	{
		
		sum(10,20); //  sum (10,20); 리턴 받지 않는 메소드
		sub(10, 20);
		mul(30,10);
		div(30,10);
		System.out.println("Lee jiyoon"); //
	}

	static void sum(int x, int y) //2
	{
		System.out.println(" 10 + 20 = "+ (x + y) );
		
	}
	static void sub(int x, int y) //2
	{
		System.out.println(" 10 - 20 = "+ (x - y) );
		
	}	
	static void mul(int x, int y) //2
	{
		System.out.println(" 30 * 10 = "+ (x * y) );
		
	}
	static void div(int x, int y) //2
	{
		System.out.println(" 30 / 10 = "+ (x/y) );
		
	}
}
