public class Arithmetic_1 
{
	public static void main(String[] args) 
	{

		int a,b,c,d,e,g;
		double f;


		a = 10;
	    b = 3;


		c=a+b;
		d=a-b;
		e=a*b;
		g=a%b;
		f=(double)a/(double)b;


		System.out.println("a+b="+c);
		System.out.println("a-b="+d);
		System.out.println("a*b="+e);
		System.out.println("a/b="+f);
		System.out.println("a/b="+f);
		System.out.println("a%b="+g);
	}
}
